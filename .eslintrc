{
  "parser": "babel-eslint",
  "env": {
    "node": true
  },
  "rules": {
    "new-cap": 0,
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "eol-last": 0,
    "quotes": [2, "single"],
    "no-unused-vars": [2, {"vars": "all", "args": "after-used"}],
    "indent": [2, 2],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "comma-dangle": [
      2,
      "always-multiline"
    ],
  }
}
