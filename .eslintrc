{
  "parser": "babel-eslint",
  "env": {
    "node": true
  },
  "extends": "./node_modules/node-style-guide/.eslintrc",
  "rules": {
    "new-cap": 0,
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "eol-last": 0,
    "quotes": [2, "single"],
    "no-unused-vars": [2, {"vars": "all", "args": "after-used"}],
    "indent": [2, 2],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "comma-dangle": [
      2,
      "always-multiline"
    ],
    "object-curly-spacing": [2, "always"],
    "curly": [2, "multi-line"],
    "no-undef": 2,
    "max-len": [2, 120, 2]
  }
}
