{
  "name": "pasta",
  "version": "0.0.1",
  "description": "Terminal App for scaffolding and creating files based on templates",
  "main": "index.js",
  "bin": {
    "pasta": "./bin/pasta"
  },
  "scripts": {
    "compile": "babel src --out-dir lib",
    "prepublish": "npm run compile",
    "test": "mocha $(find './test' -name '*-tests.js' -not -path './test/assets/*')",
    "test-watch": "mocha $(find './test' -name '*-tests.js' -not -path './test/assets/*') --watch"
  },
  "repository": "https://github.com/albertodotcom/pasta",
  "keywords": [
    "react",
    "cli",
    "node"
  ],
  "author": "Alberto Forni",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.2.1",
    "babel-preset-es2015": "^6.1.18",
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "proxyquire": "^1.7.3",
    "sinon": "^1.17.2"
  },
  "dependencies": {
    "chalk": "^1.1.1",
    "fs-extra": "^0.26.2",
    "minimist": "^1.2.0",
    "shelljs": "^0.5.3",
    "when": "^3.7.5",
    "winston": "^2.1.1"
  }
}
